


    
<div class="container col-md-6 col-sm-6 mx-auto">
    
    <h1>Welcome back</h1>
<% if @alert %>
    <p><%= @alert %></p>
<% end %>
    
<%= bootstrap_form_tag url: '/store' do |f| %>
  <%= f.text_field :username, placeholder: 'Username'%>
  <%= f.password_field :password, placeholder: 'Password' %>
  <%= f.submit "Login"%>
  Dont have an account with us? <a href="/customers/new">Create Account</a>
<% end %>



</div>